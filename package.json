{
  "name": "simplebot",
  "version": "1.0.0",
  "description": "A simple discord bot using sapphire framework",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "sapphire": "sapphire",
    "generate": "sapphire generate",
    "build": "tsc",
    "watch": "tsc -w",
    "start": "node dist/index.js",
    "dev": "npm run build && npm run start",
    "watch:start": "tsc-watch --onSuccess \"npm run start\""
  },
  "author": "EddyerDevv",
  "license": "MIT",
  "dependencies": {
    "@sapphire/decorators": "^6.1.0",
    "@sapphire/framework": "^5.0.10",
    "@sapphire/pieces": "^4.2.2",
    "@sapphire/plugin-logger": "^4.0.2",
    "@sapphire/ts-config": "^5.0.1",
    "@skyra/env-utilities": "^1.3.0",
    "colorette": "^2.0.20",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.4.3"
  }
}
